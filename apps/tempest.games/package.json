{
	"scripts": {
		"dev": "fastly compute serve --watch",
		"gen": "bun ./bun/gen-scripts.bun.ts",
		"bundle": "bun build ./src/index.tsx --outdir ./bin --external fastly:*",
		"prebuild": "bun gen && bun bundle",
		"build": "js-compute-runtime bin/index.js bin/main.wasm",
		"deploy": "fastly compute publish"
	},
	"devDependencies": {
		"@types/bun": "1.0.4",
		"htmx.org": "1.9.10",
		"hyperscript.org": "0.9.12",
		"lightningcss": "1.23.0",
		"parent-module": "3.1.0",
		"typed-htmx": "0.2.1"
	},
	"dependencies": {
		"@fastly/js-compute": "3.7.0",
		"atom.io": "0.17.0",
		"hono": "3.12.8"
	}
}
