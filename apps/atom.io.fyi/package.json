		"lint:eslint": "eslint -- .",
		"lint:types": "tsgo --noEmit"
	},
	"dependencies": {
		"@astrojs/cloudflare": "12.6.12",
		"@astrojs/mdx": "4.3.13",
		"@astrojs/preact": "4.1.3",
		"astro": "5.17.1",
		"atom.io": "workspace:*",
		"preact": "10.28.3",
		"react": "npm:@preact/compat@18.3.1",
		"react-dom": "npm:@preact/compat@18.3.1",
		"react-syntax-highlighter": "16.1.0",
		"tsdoc.json": "workspace:*"
	},
	"devDependencies": {
		"@types/bun": "npm:bun-types@1.3.9",
		"@types/react": "19.2.13",
		"@types/react-syntax-highlighter": "15.5.13",
		"chokidar": "5.0.0",
		"concurrently": "9.2.1",
		"eslint": "10.0.0",
		"takua": "workspace:*",
		"wrangler": "4.64.0"
	}
