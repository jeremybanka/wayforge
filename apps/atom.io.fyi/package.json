{
	"name": "atom.io.fyi",
	"type": "module",
	"version": "0.1.50",
	"private": true,
	"scripts": {
		"dev": "concurrently \"bun:dev:*\"",
		"dev:exhibits": "./scripts/exhibits.bun.ts watch",
		"dev:next": "next dev --turbo -p 5400",
		"build": "bun run build:tsdoc && bun run build:exhibits && bun run build:next",
		"build:tsdoc": "./scripts/tsdoc.bun.ts",
		"build:exhibits": "./scripts/exhibits.bun.ts once",
		"build:next": "next build",
		"preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
		"deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
		"upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
		"cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
		"start": "next start -p 5454",
		"lint:biome": "biome check -- .",
		"lint:eslint": "eslint -- .",
		"lint:types": "tsgo --noEmit",
		"watch:types": "tsgo --watch --noEmit",
		"lint": "concurrently \"bun:lint:*\"",
		"test": "echo no tests yet",
		"test:once": "echo no tests yet"
	},
	"engines": {
		"node": "24.10.0"
	},
	"dependencies": {
		"@opennextjs/cloudflare": "1.11.0",
		"atom.io": "workspace:*",
		"next": "15.5.6",
		"react": "19.2.0",
		"react-dom": "19.2.0",
		"react-syntax-highlighter": "15.6.6"
	},
	"devDependencies": {
		"@mdx-js/loader": "3.1.1",
		"@mdx-js/react": "3.1.1",
		"@next/mdx": "15.5.6",
		"@types/bun": "npm:bun-types@1.3.0",
		"@types/mdx": "2.0.13",
		"@types/node": "24.8.1",
		"@types/npmlog": "7.0.0",
		"@types/react": "19.2.2",
		"@types/react-dom": "19.2.2",
		"@types/react-syntax-highlighter": "15.5.13",
		"chokidar": "4.0.3",
		"concurrently": "9.2.1",
		"eslint": "9.37.0",
		"eslint-config-next": "15.5.6",
		"npmlog": "7.0.1",
		"rehype-slug": "6.0.0",
		"tsdoc.json": "workspace:*",
		"typescript": "5.9.3",
		"wrangler": "4.43.0"
	}
}
