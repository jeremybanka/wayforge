{
	"name": "atom.io.fyi",
	"type": "module",
	"version": "0.2.51",
	"private": true,
	"engines": {
		"node": "25.6.0"
	},
	"scripts": {
		"dev": "concurrently \"bun:dev:*\"",
		"dev:astro": "astro dev",
		"dev:exhibits": "./scripts/exhibits.bun.ts watch",
		"build": "bun run build:exhibits && bun run build:astro",
