{
	"name": "rel8",
	"version": "0.1.2",
	"type": "module",
	"description": "Utilities for managing relations between data.",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"module": "./dist/index.mjs",
	"files": [
		"dist",
		"junction/package.json",
		"junction/dist",
		"junction/src"
	],
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/index.d.ts",
			"browser": "./dist/index.mjs",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./junction/package.json": "./junction/package.json",
		"./junction": {
			"types": "./junction/dist/index.d.ts",
			"browser": "./junction/dist/index.mjs",
			"import": "./junction/dist/index.mjs",
			"require": "./junction/dist/index.js"
		}
	},
	"scripts": {
		"build": "tsdown",
		"lint:biome": "biome check -- .",
		"lint:eslint": "eslint -- .",
		"lint:types": "tsgo --noEmit",
		"watch:types": "tsgo --watch --noEmit",
		"lint": "concurrently \"bun:lint:*\"",
		"test": "echo 'no tests yet'",
		"test:once": "echo 'no tests yet'",
		"postversion": "biome format --write package.json"
	},
	"devDependencies": {
		"concurrently": "9.2.0",
		"eslint": "9.32.0",
		"fp-ts": "2.16.10",
		"tsdown": "0.13.2",
		"tsup": "8.5.0",
		"vite": "7.0.6",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.2.4"
	},
	"keywords": [
		"relation",
		"data",
		"relational",
		"join",
		"bridge",
		"junction"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jeremybanka/wayforge.git",
		"directory": "packages/rel8"
	},
	"license": "MIT",
	"author": "Jeremy Banka",
	"publishConfig": {
		"access": "public"
	}
}
