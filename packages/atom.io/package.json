{
	"name": "atom.io",
	"version": "0.38.2",
	"description": "Composable and testable reactive data library.",
	"homepage": "https://atom.io.fyi",
	"sideEffects": false,
	"type": "module",
	"license": "MIT",
	"author": "Jeremy Banka",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jeremybanka/wayforge.git",
		"directory": "packages/atom.io"
	},
	"scripts": {
		"manifest": "./__scripts__/manifest-build.node.ts",
		"build": "tsdown && bun ./__scripts__/build-sort.bun.ts",
		"lint:biome": "biome check -- .",
		"lint:eslint": "eslint -- .",
		"lint:types": "tsgo --noEmit",
		"watch:types": "tsgo --watch --noEmit",
		"lint": "concurrently \"bun:lint:*\"",
		"test": "vitest",
		"test:coverage": "pnpm test:coverage:once && pnpm test:coverage:increased",
		"test:coverage:once": "vitest run --coverage",
		"test:coverage:increased": "recoverage",
		"test:build": "bun run test:manifest && cross-env IMPORT=dist vitest run",
		"test:once": "echo tested built code",
		"test:once:public": "cross-env IMPORT=dist vitest run public",
		"test:manifest": "./__scripts__/manifest-test.node.ts",
		"test:semver": "break-check --verbose",
		"postversion": "biome format --write package.json",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"peerDependencies": {
		"@floating-ui/react": ">=0.25.0",
		"@floating-ui/react-dom": ">=2.0.0",
		"@testing-library/react": ">=14.0.0",
		"@typescript-eslint/parser": ">=8.0.0",
		"eslint": ">=9.0.0",
		"happy-dom": ">=17.4.4",
		"motion": ">=11.0.0",
		"react": "^18.0.0 || ^19.0.0",
		"socket.io": ">=4.0.0",
		"socket.io-client": ">=4.0.0"
	},
	"peerDependenciesMeta": {
		"@floating-ui/react": {
			"optional": true
		},
		"@floating-ui/react-dom": {
			"optional": true
		},
		"@testing-library/react": {
			"optional": true
		},
		"@typescript-eslint/parser": {
			"optional": true
		},
		"react": {
			"optional": true
		},
		"eslint": {
			"optional": true
		},
		"happy-dom": {
			"optional": true
		},
		"motion": {
			"optional": true
		},
		"socket.io": {
			"optional": true
		},
		"socket.io-client": {
			"optional": true
		}
	},
	"devDependencies": {
		"@eslint/core": "0.15.2",
		"@storybook/addon-docs": "9.1.3",
		"@storybook/addon-onboarding": "9.1.3",
		"@storybook/react-vite": "9.1.3",
		"@testing-library/react": "16.3.0",
		"@types/bun": "npm:bun-types@1.2.20",
		"@types/eslint": "9.6.1",
		"@types/estree": "1.0.8",
		"@types/http-proxy": "1.17.16",
		"@types/npmlog": "7.0.0",
		"@types/react": "19.1.11",
		"@types/tmp": "0.2.6",
		"@typescript-eslint/parser": "8.40.0",
		"@typescript-eslint/rule-tester": "8.40.0",
		"@typescript-eslint/utils": "8.40.0",
		"@vitest/coverage-v8": "3.2.4",
		"@vitest/ui": "3.2.4",
		"break-check": "workspace:*",
		"concurrently": "9.2.0",
		"drizzle-kit": "0.31.4",
		"drizzle-orm": "0.44.4",
		"eslint": "9.34.0",
		"happy-dom": "18.0.1",
		"http-proxy": "1.18.1",
		"motion": "12.23.12",
		"npmlog": "7.0.1",
		"nyc": "17.1.0",
		"postgres": "3.4.7",
		"preact": "10.27.1",
		"react": "19.1.1",
		"react-dom": "19.1.1",
		"react-router-dom": "7.8.2",
		"recoverage": "0.1.11",
		"socket.io": "4.8.1",
		"socket.io-client": "4.8.1",
		"storybook": "9.1.3",
		"tmp": "0.2.5",
		"tsdown": "0.14.1",
		"tsx": "4.20.4",
		"typescript": "5.9.2",
		"vite": "7.1.3",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.2.4",
		"zod": "3.25.76"
	},
	"main": "./dist/main/index.js",
	"files": [
		"dist",
		"src"
	],
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": "./dist/main/index.js",
			"types": "./dist/main/index.d.ts"
		},
		"./data": {
			"import": "./dist/data/index.js",
			"types": "./dist/data/index.d.ts"
		},
		"./eslint-plugin": {
			"import": "./dist/eslint-plugin/index.js",
			"types": "./dist/eslint-plugin/index.d.ts"
		},
		"./internal": {
			"import": "./dist/internal/index.js",
			"types": "./dist/internal/index.d.ts"
		},
		"./introspection": {
			"import": "./dist/introspection/index.js",
			"types": "./dist/introspection/index.d.ts"
		},
		"./json": {
			"import": "./dist/json/index.js",
			"types": "./dist/json/index.d.ts"
		},
		"./react": {
			"import": "./dist/react/index.js",
			"types": "./dist/react/index.d.ts"
		},
		"./react-devtools": {
			"import": "./dist/react-devtools/index.js",
			"types": "./dist/react-devtools/index.d.ts"
		},
		"./react-devtools/css": {
			"import": "./dist/react-devtools/index.css"
		},
		"./realtime": {
			"import": "./dist/realtime/index.js",
			"types": "./dist/realtime/index.d.ts"
		},
		"./realtime-client": {
			"import": "./dist/realtime-client/index.js",
			"types": "./dist/realtime-client/index.d.ts"
		},
		"./realtime-react": {
			"import": "./dist/realtime-react/index.js",
			"types": "./dist/realtime-react/index.d.ts"
		},
		"./realtime-server": {
			"import": "./dist/realtime-server/index.js",
			"types": "./dist/realtime-server/index.d.ts"
		},
		"./realtime-testing": {
			"import": "./dist/realtime-testing/index.js",
			"types": "./dist/realtime-testing/index.d.ts"
		},
		"./transceivers/set-rtx": {
			"import": "./dist/transceivers/set-rtx/index.js",
			"types": "./dist/transceivers/set-rtx/index.d.ts"
		},
		"./web": {
			"import": "./dist/web/index.js",
			"types": "./dist/web/index.d.ts"
		}
	}
}
