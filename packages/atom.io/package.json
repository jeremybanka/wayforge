{
	"name": "atom.io",
	"version": "0.33.2",
	"description": "Composable and testable reactive data library.",
	"homepage": "https://atom.io.fyi",
	"sideEffects": false,
	"type": "module",
	"license": "MIT",
	"author": "Jeremy Banka",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jeremybanka/wayforge.git",
		"directory": "packages/atom.io"
	},
	"scripts": {
		"manifest": "tsx __scripts__/manifest-build.node.ts",
		"build": "tsdown && bun ./__scripts__/build-sort.bun.ts",
		"lint:biome": "biome check -- .",
		"lint:eslint": "eslint -- .",
		"lint:types": "tsc --noEmit",
		"watch:types": "tsc --watch --noEmit",
		"lint": "concurrently \"bun:lint:*\"",
		"test": "vitest",
		"test:coverage": "pnpm test:coverage:once && pnpm test:coverage:increased",
		"test:coverage:once": "vitest run --coverage",
		"test:coverage:increased": "recoverage",
		"test:build": "bun run test:manifest && cross-env IMPORT=dist vitest run",
		"test:once": "echo tested built code",
		"test:once:public": "cross-env IMPORT=dist vitest run public",
		"test:manifest": "tsx __scripts__/manifest-test.node.ts",
		"test:semver": "break-check --verbose",
		"postversion": "biome format --write package.json"
	},
	"peerDependencies": {
		"@floating-ui/react": ">=0.25.0",
		"@floating-ui/react-dom": ">=2.0.0",
		"@testing-library/react": ">=14.0.0",
		"@typescript-eslint/parser": ">=9.0.0",
		"eslint": ">=9.0.0",
		"happy-dom": ">=17.4.4",
		"motion": ">=11.0.0",
		"react": "^18.0.0 || ^19.0.0",
		"socket.io": ">=4.0.0",
		"socket.io-client": ">=4.0.0"
	},
	"peerDependenciesMeta": {
		"@floating-ui/react": {
			"optional": true
		},
		"@floating-ui/react-dom": {
			"optional": true
		},
		"@testing-library/react": {
			"optional": true
		},
		"@typescript-eslint/parser": {
			"optional": true
		},
		"react": {
			"optional": true
		},
		"eslint": {
			"optional": true
		},
		"happy-dom": {
			"optional": true
		},
		"motion": {
			"optional": true
		},
		"socket.io": {
			"optional": true
		},
		"socket.io-client": {
			"optional": true
		}
	},
	"devDependencies": {
		"@eslint/core": "0.14.0",
		"@testing-library/react": "16.3.0",
		"@types/bun": "npm:bun-types@1.2.12",
		"@types/eslint": "9.6.1",
		"@types/estree": "1.0.6",
		"@types/http-proxy": "1.17.16",
		"@types/npmlog": "7.0.0",
		"@types/react": "19.1.3",
		"@types/tmp": "0.2.6",
		"@typescript-eslint/parser": "8.32.0",
		"@typescript-eslint/rule-tester": "8.32.0",
		"@typescript-eslint/utils": "8.32.0",
		"@vitest/coverage-v8": "3.1.3",
		"@vitest/ui": "3.1.3",
		"break-check": "workspace:*",
		"concurrently": "9.1.2",
		"drizzle-kit": "0.31.1",
		"drizzle-orm": "0.43.1",
		"eslint": "9.26.0",
		"happy-dom": "17.4.6",
		"http-proxy": "1.18.1",
		"motion": "12.10.3",
		"npmlog": "7.0.1",
		"nyc": "17.1.0",
		"postgres": "3.4.5",
		"preact": "10.26.6",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-router-dom": "7.5.3",
		"recoverage": "0.1.9",
		"socket.io": "4.8.1",
		"socket.io-client": "4.8.1",
		"tmp": "0.2.3",
		"tsdown": "0.11.1",
		"tsx": "4.19.4",
		"typescript": "5.8.3",
		"vite": "6.3.5",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.1.3",
		"zod": "3.24.4"
	},
	"main": "./dist/main/index.js",
	"files": [
		"dist",
		"src",
		"data/src",
		"eslint-plugin/src",
		"internal/src",
		"introspection/src",
		"json/src",
		"react/src",
		"react-devtools/src",
		"realtime/src",
		"realtime-client/src",
		"realtime-react/src",
		"realtime-server/src",
		"realtime-testing/src",
		"transceivers/set-rtx/src",
		"web/src"
	],
	"exports": {
		"./package.json": "./package.json",
		".": {
			"import": "./dist/main/index.js",
			"types": "./dist/main/index.d.ts"
		},
		"./data": {
			"import": "./dist/data/index.js",
			"types": "./dist/data/index.d.ts"
		},
		"./eslint-plugin": {
			"import": "./dist/eslint-plugin/index.js",
			"types": "./dist/eslint-plugin/index.d.ts"
		},
		"./internal": {
