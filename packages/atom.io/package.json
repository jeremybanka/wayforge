{
	"name": "atom.io",
	"version": "0.25.3",
	"description": "Composable and testable reactive data library.",
	"homepage": "https://atom.io.fyi",
	"sideEffects": false,
	"type": "module",
	"license": "MIT",
	"author": "Jeremy Banka",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jeremybanka/wayforge.git",
		"directory": "packages/atom.io"
	},
	"scripts": {
		"manifest": "tsx __scripts__/manifest-build.node.ts",
		"clean:build": "find . -type d -name 'dist' -not -path '*/node_modules/*' | xargs rm -rf",
		"build": "bun run clean:build && concurrently \"bun:build:*\"",
		"build:main": "tsup",
		"lint:eslint:build": "bun run build:main",
		"lint:types": "tsc --noEmit",
		"lint": "bun run lint:biome && bun run lint:eslint && bun run lint:types",
		"test": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:once": "bun run test:manifest && cross-env IMPORT=dist vitest run",
		"test:once:public": "cross-env IMPORT=dist vitest run public",
		"test:manifest": "tsx __scripts__/manifest-test.node.ts",
		"test:semver": "bun ../break-check/src/break-check.x.ts --verbose"
	},
	"peerDependencies": {
		"@floating-ui/react": ">=0.25.0",
		"@floating-ui/react-dom": ">=2.0.0",
		"@testing-library/react": ">=14.0.0",
		"framer-motion": ">=10.0.0",
		"react": ">=18.0.0",
		"socket.io": ">=4.0.0",
		"socket.io-client": ">=4.0.0"
	},
	"peerDependenciesMeta": {
		"@testing-library/react": {
			"optional": true
		},
		"react": {
			"optional": true
		},
		"@floating-ui/react": {
			"optional": true
		},
		"@floating-ui/react-dom": {
			"optional": true
		},
		"framer-motion": {
			"optional": true
		},
		"socket.io": {
			"optional": true
		},
		"socket.io-client": {
			"optional": true
		}
	},
	"devDependencies": {
		"@testing-library/react": "16.0.0",
		"@types/eslint": "npm:@types/eslint@8.56.10",
		"@types/eslint-v9": "npm:@types/eslint@8.56.10",
		"@types/estree": "1.0.5",
		"@types/http-proxy": "1.17.14",
		"@types/npmlog": "7.0.0",
		"@types/react": "18.3.3",
		"@types/tmp": "0.2.6",
		"@typescript-eslint/parser": "7.15.0",
		"@typescript-eslint/rule-tester": "7.15.0",
		"@vitest/coverage-v8": "1.6.0",
		"@vitest/ui": "1.6.0",
		"concurrently": "8.2.2",
		"drizzle-kit": "0.22.8",
		"drizzle-orm": "0.31.2",
		"eslint": "npm:eslint@8.57.0",
		"eslint-v9": "npm:eslint@9.6.0",
		"framer-motion": "11.2.12",
		"happy-dom": "14.12.3",
		"http-proxy": "1.18.1",
		"npmlog": "7.0.1",
		"postgres": "3.4.4",
		"preact": "10.22.1",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"react-router-dom": "6.24.1",
		"socket.io": "4.7.5",
		"socket.io-client": "4.7.5",
		"tmp": "0.2.3",
		"tsup": "8.1.0",
		"typescript": "5.5.3",
		"vite": "5.3.3",
		"vite-tsconfig-paths": "4.3.2",
		"vitest": "1.6.0"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"src",
		"data/package.json",
		"data/src",
		"ephemeral/package.json",
		"ephemeral/src",
		"eslint-plugin/package.json",
		"eslint-plugin/src",
		"immortal/package.json",
		"immortal/src",
		"internal/package.json",
		"internal/src",
		"introspection/package.json",
		"introspection/src",
		"json/package.json",
		"json/src",
		"react/package.json",
		"react/src",
		"react-devtools/package.json",
		"react-devtools/src",
		"realtime/package.json",
		"realtime/src",
		"realtime-client/package.json",
		"realtime-client/src",
		"realtime-react/package.json",
		"realtime-react/src",
		"realtime-server/package.json",
		"realtime-server/src",
		"realtime-testing/package.json",
		"realtime-testing/src",
		"transceivers/set-rtx/package.json",
		"transceivers/set-rtx/src",
		"eslint-plugin/dist"
	],
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts"
		},
		"./data/package.json": "./data/package.json",
		"./data": {
			"types": "./data/src/index.ts",
			"import": "./data/src/index.ts"
		},
		"./ephemeral/package.json": "./ephemeral/package.json",
		"./ephemeral": {
			"types": "./ephemeral/src/index.ts",
			"import": "./ephemeral/src/index.ts"
		},
		"./eslint-plugin/package.json": "./eslint-plugin/package.json",
		"./eslint-plugin": {
			"import": "./eslint-plugin/dist/index.js",
			"require": "./eslint-plugin/dist/index.cjs"
		},
		"./immortal/package.json": "./immortal/package.json",
		"./immortal": {
			"types": "./immortal/src/index.ts",
			"import": "./immortal/src/index.ts"
		},
		"./internal/package.json": "./internal/package.json",
		"./internal": {
			"types": "./internal/src/index.ts",
			"import": "./internal/src/index.ts"
		},
		"./introspection/package.json": "./introspection/package.json",
		"./introspection": {
			"types": "./introspection/src/index.ts",
			"import": "./introspection/src/index.ts"
		},
		"./json/package.json": "./json/package.json",
		"./json": {
			"types": "./json/src/index.ts",
			"import": "./json/src/index.ts"
		},
		"./react/package.json": "./react/package.json",
		"./react": {
			"types": "./react/src/index.ts",
			"import": "./react/src/index.ts"
		},
		"./react-devtools/package.json": "./react-devtools/package.json",
		"./react-devtools": {
			"types": "./react-devtools/src/index.ts",
			"import": "./react-devtools/src/index.ts"
		},
		"./realtime/package.json": "./realtime/package.json",
		"./realtime": {
			"types": "./realtime/src/index.ts",
			"import": "./realtime/src/index.ts"
		},
		"./realtime-client/package.json": "./realtime-client/package.json",
		"./realtime-client": {
			"types": "./realtime-client/src/index.ts",
			"import": "./realtime-client/src/index.ts"
		},
		"./realtime-react/package.json": "./realtime-react/package.json",
		"./realtime-react": {
			"types": "./realtime-react/src/index.ts",
			"import": "./realtime-react/src/index.ts"
		},
		"./realtime-server/package.json": "./realtime-server/package.json",
		"./realtime-server": {
			"types": "./realtime-server/src/index.ts",
			"import": "./realtime-server/src/index.ts"
		},
		"./realtime-testing/package.json": "./realtime-testing/package.json",
		"./realtime-testing": {
			"types": "./realtime-testing/src/index.ts",
			"import": "./realtime-testing/src/index.ts"
		},
		"./transceivers/set-rtx/package.json": "./transceivers/set-rtx/package.json",
		"./transceivers/set-rtx": {
			"types": "./transceivers/set-rtx/src/index.ts",
			"import": "./transceivers/set-rtx/src/index.ts"
		}
	}
}
