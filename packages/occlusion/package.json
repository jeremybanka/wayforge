		"tsup": "8.4.0",
		"typescript": "5.8.2",
		"vite": "6.2.4",
		"vite-tsconfig-paths": "5.1.4",
		"vitest": "3.1.1"
	},
	"peerDependencies": {
		"fp-ts": "^2.13.1",
		"io-ts": "^2.2.20"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jeremybanka/wayforge.git",
		"directory": "packages/occlusion"
	},
