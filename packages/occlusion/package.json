{
	"name": "occlusion",
	"version": "0.0.2",
	"type": "module",
	"description": "it's a matter of perspective",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": ["dist"],
	"scripts": {
		"build": "tsup src/index.ts --format cjs,esm --dts",
		"lint:biome": "biome check -- .",
		"lint:eslint": "eslint -- .",
