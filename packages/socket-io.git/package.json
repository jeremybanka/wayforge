{
  "name": "socket-io.git",
  "version": "0.1.1",
  "description": "remote control a simple-git instance over a socket.io connection",
  "files": [
    "dist",
    "node",
    "web",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsup --entry src/socket-git-node.ts --entry src/socket-git-recoil.ts --format cjs,esm --dts --external react,recoil,socket.io,socket.io-client",
    "lint": "eslint",
    "test": "vitest",
    "test:once": "vitest run"
  },
  "packageManager": "yarn@3.4.1",
  "peerDependencies": {
    "react": "^18.2.0",
    "recoil": "^0.7.6",
    "socket.io": "^4.6.0",
    "socket.io-client": "^4.6.0"
  },
  "dependencies": {
    "fp-ts": "2.13.1",
    "simple-git": "3.17.0"
  },
  "devDependencies": {
    "react": "18.2.0",
    "recoil": "0.7.7",
    "socket.io": "4.6.1",
    "socket.io-client": "4.6.1",
    "tsup": "6.6.3",
    "vitest": "0.29.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeremybanka/wayforge.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "author": "Jeremy Banka",
  "publishConfig": {
    "access": "public"
  }
}
