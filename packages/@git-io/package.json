{
  "name": "socket-io.git",
  "version": "0.0.0",
  "description": "remote control a simple-git instance over a socket.io connection",
  "files": [
    "*"
  ],
  "scripts": {
    "build": "tsup --entry src/git-io-node.ts --entry src/git-io-web.ts --format cjs,esm --dts --external react,recoil,socket.io,socket.io-client"
  },
  "dependencies": {
    "fp-ts": "2.13.1",
    "simple-git": "3.16.1"
  },
  "devDependencies": {
    "react": "18.2.0",
    "recoil": "0.7.6",
    "recoil-sync": "0.2.0",
    "socket.io": "4.6.0",
    "socket.io-client": "4.6.0",
    "tsup": "6.6.3",
    "vitest": "0.28.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "recoil": "0.7.6",
    "socket.io": "4.6.0",
    "socket.io-client": "4.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeremybanka/wayforge.git"
