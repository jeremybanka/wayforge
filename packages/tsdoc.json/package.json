{
	"name": "tsdoc.json",
	"version": "0.6.6",
	"license": "MIT",
	"author": {
		"name": "Jeremy Banka",
		"email": "hello@jeremybanka.com"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jeremybanka/wayforge.git",
		"directory": "packages/tsdoc.json"
	},
	"type": "module",
	"files": [
		"dist",
		"src"
	],
	"exports": {
		".": {
			"types": "./dist/main/index.d.ts",
			"import": "./dist/main/index.js",
			"browser": "./dist/main/index.js"
		},
		"./react": {
			"types": "./dist/react/index.d.ts",
			"import": "./dist/react/index.js",
			"browser": "./dist/react/index.js"
		}
	},
	"main": "dist/main/index.js",
	"types": "dist/main/index.d.ts",
	"dependencies": {
		"@microsoft/tsdoc": "0.16.0",
		"colors": "1.4.0"
	},
	"scripts": {
		"build": "tsdown",
		"lint:biome": "biome check -- .",
		"lint:eslint": "eslint -- .",
		"lint:types": "tsc --noEmit",
		"watch:types": "tsc --watch --noEmit",
		"lint": "concurrently \"bun:lint:*\"",
		"test": "bun test --watch -- **/*.test.ts",
		"test:once": "bun test -- **/*.test.ts",
		"postversion": "biome format --write package.json"
	},
	"devDependencies": {
		"@types/bun": "npm:bun-types@1.3.2",
		"@types/react": "19.2.5",
		"concurrently": "9.2.1",
		"eslint": "9.39.1",
		"react": "19.2.0",
		"rimraf": "6.1.0",
		"tmp": "0.2.5",
		"tsdown": "0.16.4",
		"typescript": "5.9.3"
	}
}
