{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"types": ["vitest/globals"],
		"noEmit": true,
		"paths": {
			"hamr/*": ["./src/*"],
			"atom.io": ["../atom.io/src/main"],
			"atom.io/*": ["../atom.io/src/*"]
		}
	},
	"include": ["**/src", "__scripts__", "__experimental__", "**/*.config.ts"]
}
