	"devDependencies": {
		"concurrently": "9.2.1",
		"eslint": "9.39.2",
		"tsdown": "0.20.2",
		"vite": "7.3.1",
		"vite-tsconfig-paths": "6.0.5",
		"vitest": "4.0.18"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/jeremybanka/wayforge.git",
		"directory": "packages/anvl"
	},
	"license": "MIT",
	"sideEffects": false,
	"author": "Jeremy Banka",
	"publishConfig": {
		"access": "public"
	}
}
